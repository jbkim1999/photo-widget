<br><br>
<%= form_with url: "/", method: :get do |form| %>
  <%= form.label :user_id, "User ID " %>
  <%= form.text_field :user_id %>
  <br><br>  
  <%= form.submit "Look for the photos" %>
<% end %>
<br><br>

<% unless @photos.nil? %>
  <% @photos.each do |photo| %>
    <% photo_url = "https://live.staticflickr.com/#{photo.server}/#{photo.id}_#{photo.secret}_m.jpg" %>
    <img src=<%= photo_url%>>
  <% end %>
<% end %>